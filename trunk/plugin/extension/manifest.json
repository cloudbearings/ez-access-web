{
  "name": "EZ Access Browser",
  "description": "An EZ Access Web Parser",
  "version": "0.9.6.1",
  "manifest_version": 9,
  "icons": { 
    "16":"icon-16.png", 
    "48":"icon-48.png", 
    "128":"icon-128.png" 
  }, 
  "permissions": ["tts"],
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "page_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "EZ Access",
    "default_popup": "popup.html"
  },
  "content_scripts" : [
    {
      "run_at": "document_end",
      "matches" : [
        "*://*/*"
      ],
      "css": ["style.css"],
      "js" : ["enable.js","settings.json","js/ezaccess.js","js/hammer.js","js/tinybox.js"]
    }
  ],
  "web_accessible_resources": [
    "earcons/*",
    "trace_logo.jpg"
  ]
}